{
  "providers": {
    "claude": {
      "enabled": false,
      "api_key": "mock-claude-key",
      "base_url": "https://api.anthropic.com",
      "api_version": "2023-06-01"
    },
    "openai": {
      "enabled": true,
      "api_key": "sk-test-mock-key",
      "base_url": "http://localhost:8001",
      "organization": null
    },
    "azure": {
      "enabled": false,
      "api_key": "mock-azure-key",
      "endpoint": "https://mock-azure.openai.azure.com",
      "api_version": "2024-10-21"
    }
  },
  "model_mapping": {
    "gpt-4o": "openai",
    "gpt-4o-mini": "openai",
    "gpt-4": "openai",
    "gpt-4-turbo": "openai",
    "gpt-3.5-turbo": "openai"
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "log_level": "INFO",
    "workers": 1
  },
  "features": {
    "enable_streaming": true,
    "enable_caching": false,
    "enable_metrics": true,
    "enable_rate_limiting": false,
    "max_requests_per_minute": 60
  },
  "security": {
    "enable_auth": false,
    "api_key_header": "X-API-Key",
    "allowed_origins": ["*"],
    "enable_cors": true
  }
}