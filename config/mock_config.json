{
  "providers": {
    "mock": {
      "enabled": true,
      "api_key": "mock-key",
      "base_url": "https://mock.example.com"
    }
  },
  "model_mapping": {
    "gpt-4o": "mock",
    "gpt-4o-mini": "mock",
    "gpt-3.5-turbo": "mock"
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "log_level": "INFO",
    "workers": 1
  },
  "features": {
    "enable_streaming": true,
    "enable_caching": false,
    "enable_metrics": true,
    "enable_rate_limiting": false,
    "max_requests_per_minute": 60
  },
  "security": {
    "enable_auth": false,
    "api_key_header": "X-API-Key",
    "allowed_origins": ["*"],
    "enable_cors": true
  }
}